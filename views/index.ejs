<link rel="stylesheet" href="/css/index.css">
<a href="/addaccount">Thêm tài khoản</a>
<table>
    <thead>
        <tr>  
        <th>Account</th>
        <th>Email</th>
        <th>Username</th>
        <th>Fullname</th>
        <th>DepartmentId</th>
        <th>PositionId</th>
        <th>createdate</th>
    </tr>
      
    </thead>
    
        <% account.forEach(account=>{  %>
            <tbody>

                    <td><%=  account.AccountID %></td>
                    <td>  <%= account.Email %></td>
                    <td>  <%= account.Username %></td>
                    <td>  <%= account.FullName %></td>
                    <td> <%=  account.DepartmentID %></td>
                    <td>  <%= account.PositionID %></td>
                    <td><%= account.CreateDate %></td>
            </tbody>
        
        <%    }); %>
      
</table>
